{
  "name": "ual-scatter",
  "version": "10.3.0",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Block.one",
    "url": "https://block.one"
  },
  "contributors": [
    "Nasser Abouelazm",
    "Chris Allnutt",
    "Randy Torres",
    "Jeffrey Smith II"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prepare": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@scatterjs/core": "^2.7.54",
    "@scatterjs/eosjs2": "^1.5.36",
    "elliptic": "^6.5.4",
    "eosjs": "^22.1.0",
    "universal-authenticator-library": "^0.3.0"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist"
    ],
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/runtime": "^7.15.3",
    "@blockone/eslint-config-blockone": "^4.0.2",
    "@types/elliptic": "^6.4.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/ual-scatter"
  }
}
